<div class="wrapper-grey padded">
  <div class="container">
    <div class="card-header">
      <h2 class="card-title">Your Incoming Booking Requests 🚀</h2>
    </div>
    <br>
    <% if current_user.spaces.present? %>
    <% current_user.spaces.each do |space| %>
    <% space.bookings.each do |booking| %>
    <% if booking.present? %>
    <div class="card card-aside">
      <a href="#" class="card-aside-column w-50" style="background-image: url( <%= cl_image_path booking.space.pictures[0].path if booking.space.pictures.any? %>)"></a>
      <div class="card-body d-flex flex-column">
        <h4><a href="#"><%= link_to booking.space.name, space_path(booking.space)%></a></h4>
        <div class="text-muted">
        </div>
        <div class="d-flex align-items-center pt-5 mt-auto">
          <ul class="list-inline product-controls hidden-sm hidden-xs">
            <% if booking.status == true %>
            <p class="status accepted">Accepted</p>
            <% elsif booking.status == false %>
            <p class="status declined">Declined</p>
            <% else %>
            <%= simple_form_for ([space, booking]) do |f| %>
            <%= f.select :status, [['Accepted', true],['Declined', false]]%>
            <br>
            <br>
            <%= f.button :submit, :class => 'btn btn-primary' %>
            <% end %>
            <% end %>
          </ul>
          <div class="ml-auto text-muted">
            <!-- <a href="javascript:void(0)" class="icon d-none d-md-inline-block ml-3"><i class="fe fe-heart mr-1"></i></a> -->
          </div>
        </div>
      </div>
    </div>
    <% end %>
    <% end %>
    <% end %>
    <% end %>
  </div>
</div>


<div class="wrapper-grey padded">
  <div class="container">
    <div class="card-header">
      <h2 class="card-title">My Outgoing Requests 🚀</h2>
    </div>
    <br>
    <% Booking.all.each do |booking| %>
    <% if booking.user == current_user %>
    <div class="card card-aside">
      <a href="#" class="card-aside-column w-50" style="background-image: url( <%= cl_image_path booking.space.pictures[0].path if booking.space.pictures.any? %>)"></a>
      <div class="card-body d-flex flex-column">
        <h4><a href="#"><%= link_to booking.space.name, space_path(booking.space)%></a></h4>
        <div class="text-muted">
          <% if booking.end_time < Date.today %>
          <p>
            <%= link_to "Review this past booking ❤️", new_booking_review_path(booking) %>
          </p>
          <% else %>
          <%#= raise %>
          <p><%= booking.start_time%> - <%= booking.end_time %></p>
          <% end %>
        </div>
        <div class="d-flex align-items-center pt-5 mt-auto">
          <ul class="list-inline product-controls hidden-sm hidden-xs">
            <% if booking.status.nil? %>
            <p class="status pending">Pending</p>
            <% elsif booking.status == true %>
            <p class="status accepted">Accepted</p>
            <% elsif booking.status == false %>
            <p class="status declined">Declined</p>
            <% end %>
          </ul>
          <div class="ml-auto text-muted">
            <!-- <a href="javascript:void(0)" class="icon d-none d-md-inline-block ml-3"><i class="fe fe-heart mr-1"></i></a> -->
          </div>
        </div>
      </div>
    </div>
    <% end %>
    <% end %>
  </div>
</div>

<div class="wrapper-grey padded">
  <div class="container">
    <div class="card">
      <div class="card-header">
        <h3 class="card-title">My Profile</h3>
      </div>
      <div class="card-body">
        <form>
          <div class="row">
            <div class="col-auto">
              <span class="avatar avatar-xl" style="background-image: url(<%= image_path(current_user.avatar) %>)"></span>
            </div>
            <div class="col">
              <div class="form-group">
                <label class="form-label">Email-Address</label>
                <input class="form-control" placeholder="<%= current_user.email %>">
              </div>
            </div>
          </div>
          <div class="form-group">
            <label class="form-label">First Name</label>
            <input class="form-control" placeholder=<%= current_user.first_name %>>
          </div>
          <div class="form-group">
            <label class="form-label">Last Name</label>
            <input class="form-control" placeholder=<%= current_user.last_name %>>
          </div>
          <div class="form-group">
            <label class="form-label">Bio</label>
            <textarea class="form-control" rows="5"><%= current_user.bio %></textarea>
          </div>
          <div class="form-group">
            <label class="form-label">Password</label>
            <input type="password" class="form-control" value="password">
            <span style="opacity: 1; background-size: 19px 13px; left: 648px; top: 402.5px; width: 19px; min-width: 19px; height: 13px; position: absolute; background-image: url(); background-repeat: no-repeat; background-position: 0px 0px; border: none; display: inline; visibility: visible; z-index: auto;"></span></div>
            <div class="form-footer">
              <button class="btn btn-primary btn-block">Save</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>

  <div class="wrapper-grey padded">
    <div class="container">
      <div class="card-header">
        <h2 class="card-title">My Spaces</h2>
      </div>
      <br>
      <% current_user.spaces.each do |space| %>
      <div class="card card-aside">
        <a href="#" class="card-aside-column w-50" style="background-image: url( <%= cl_image_path space.pictures[0].path if space.pictures.any? %>)"></a>
        <div class="card-body d-flex flex-column">
          <h4><a href="#"><%= link_to space.name, space_path(space)%></a></h4>
          <div class="text-muted"><%= space.description %></div>
          <div class="d-flex align-items-center pt-5 mt-auto">
            <div class="avatar avatar-md mr-3" style="background-image: url(<%= image_path current_user.avatar %>)"></div>
            <div>
              <a href="./profile.html" class="text-default"><%= current_user.first_name %> <%= current_user.last_name %></a>
              <!-- <small class="d-block text-muted">3 days ago</small> -->
            </div>
            <div class="ml-auto text-muted">
              <!-- <a href="javascript:void(0)" class="icon d-none d-md-inline-block ml-3"><i class="fe fe-heart mr-1"></i></a> -->
            </div>
          </div>
        </div>
      </div>
      <% end %>
    </div>
  </div>

